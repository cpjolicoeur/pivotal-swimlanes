    "lodash": "^4.17.4",
    "material-ui": "^0.16.7",
    "react": "^15.4.2",
    "react-autocomplete": "^1.4.0",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-router": "^3.0.2",
